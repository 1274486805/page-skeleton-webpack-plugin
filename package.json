{
  "name": "page-skeleton-webpack-plugin",
  "version": "0.0.4",
  "description": "web site skeleton generator",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src client/{index,sockjs,utils}.js --fix",
    "dev:preview": "webpack --color --config client/webpack.preview.config.js --watch",
    "dev:client": "webpack ./client/index.js client/index.bundle.js --color --config client/webpack.config.js --watch",
    "build:index": "webpack ./client/index.js client/index.bundle.js --color --config client/webpack.config.js",
    "build:sockjs": "webpack ./client/sockjs.js client/sockjs.bundle.js --color --config client/webpack.sockjs.config.js",
    "build:preview": "webpack --color --config client/webpack.preview.config.js",
    "prepare": "npm run build:index && npm run build:sockjs && npm run build:preview"
  },
  "keywords": [
    "puppeteer",
    "page skeleton",
    "webpack"
  ],
  "author": "jocs",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.3.0",
    "css-tree": "^1.0.0-alpha25",
    "csso": "^3.3.1",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "hasha": "^3.0.0",
    "html-minifier": "^3.5.6",
    "imagemin": "^5.3.1",
    "imagemin-pngquant": "^5.0.1",
    "memory-fs": "^0.4.1",
    "opn": "^5.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "postcss-nested": "^2.1.2",
    "puppeteer": "^0.12.0",
    "raw-loader": "^0.5.1",
    "sockjs": "^0.3.19",
    "sockjs-client": "^1.1.4",
    "tiza": "^2.1.0",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "vue": "^2.5.2",
    "vue-style-loader": "^3.0.3",
    "webpack": "^3.8.1"
  }
}
