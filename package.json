{
  "name": "skeleton",
  "version": "0.0.1",
  "description": "web site skeleton generator",
  "main": "index.js",
  "scripts": {
    "build:index": "webpack ./client/index.js client/index.bundle.js --color --config client/webpack.config.js",
    "build:sockjs": "webpack ./client/sockjs.js client/sockjs.bundle.js --color --config client/webpack.sockjs.config.js",
    "prepublish": "npm run build:index && npm run build:sockjs"
  },
  "keywords": [
    "puppeteer"
  ],
  "author": "jocs",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.3.0",
    "express": "^4.16.2",
    "fs-extra": "^4.0.2",
    "hasha": "^3.0.0",
    "html-minifier": "^3.5.6",
    "memory-fs": "^0.4.1",
    "opn": "^5.1.0",
    "puppeteer": "^0.12.0",
    "sockjs": "^0.3.19",
    "sockjs-client": "^1.1.4",
    "tiza": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.8.0",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "vue": "^2.5.2",
    "webpack": "^3.8.1"
  }
}
